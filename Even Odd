#include <iostream>
using namespace std;

int main()
{
    int x = 1;
    cout << "Enter a number to check if odd or even:\n";
    cin >> x;
    cout << endl;

    if (x % 2 == 0) {
        cout << x << " is Even";
    }
    else if (x % 2 == 1) {
        cout << x << " is Odd";
    }
    else {
        cout << "Incorrect Input";
    }
}
